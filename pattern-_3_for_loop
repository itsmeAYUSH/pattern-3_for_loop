package com.company;

public class pattern4 {
    public static void main(String args[]){
        for(int i=1 ; i<= 5 ; i ++){
            String s = "";
            for(int  j = 10 ; j <= 15-i ; j ++ ){
                s = s+"*";
            }for(int k = 10 ; k <= (2*i)+8 ; k++){
                s = s+" ";
            }for(int  j = 10 ; j <= 15-i ; j ++ ){
                s = s+"*";
            }
            System.out.println(s);
        }
    }
}
